{
  "name": "react-demo",
  "version": "1.0.0",
  "type": "module",
  "description": "A React demo project with Vite",
  "scripts": {
    "preview": "vite preview",
    "prepare": "husky",
    "dev": "vite --mode dev",
    "build": "pnpm install && vite build --mode test",
    "build:prod": "pnpm install && vite build --mode prod",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "hotUpdate:test": "node ./scripts/release/release.cjs --mode test",
    "release:test": "pnpm install && node ./scripts/release/setVersion.cjs && npm run build && npm run hotUpdate:test && node ./scripts/release/updateVersion.cjs --mode test",
    "update": "node ./scripts/release/setVersion.cjs --mode test"
  },
  "dependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@sentry/browser": "^10.5.0",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "ahooks": "^3.9.0",
    "antd": "^5.27.0",
    "axios": "^1.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^11.1.0",
    "vite-plugin-pwa": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.3.0",
    "@eslint/js": "^9.33.0",
    "@types/node": "^20.14.9",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-legacy": "^5.3.0",
    "@welldone-software/why-did-you-render": "^10.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chalk": "^5.5.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "regenerator-runtime": "^0.14.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^3.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
